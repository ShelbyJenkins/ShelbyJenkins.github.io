{
	"name": "personal blog",
	"image": "mcr.microsoft.com/devcontainers/mcr.microsoft.com/devcontainers/base:ubuntu",
	"features": {
		"ghcr.io/devcontainers/features/common-utils:2": {"version": "latest"},
		"ghcr.io/devcontainers/features/node:1": {"version": "latest"}
	},
    "remoteUser": "root",
    "customizations": {
		"vscode": {
			"extensions": [
				"github.vscode-pull-request-github",
				"github.vscode-github-actions",
				"github.copilot",
				"github.copilot-chat",
				"d-koppenhagen.file-tree-to-text-generator",
				"astro-build.astro-vscode"
			]
		}
	}
}
